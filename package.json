{
  "name": "mineball-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "mineball - A flywheel-style meme coin on Solana",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "anchor:build": "cd programs/mineball && anchor build",
    "anchor:test": "cd programs/mineball && anchor test",
    "anchor:deploy": "cd programs/mineball && anchor deploy"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.1.0",
    "turbo": "^1.11.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@8.12.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
